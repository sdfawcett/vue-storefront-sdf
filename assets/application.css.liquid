/* MENU DRAWER CARD */

.md-card {
    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
    max-width: 300px;
    margin: auto;
    text-align: center;
    font-family: arial;
    padding: 1rem;
    font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
}

.md-price {
    color: grey;
    font-size: 22px;
    margin-bottom: 1rem;
}

.md-card button {
    border: none;
    outline: 0;
    padding: 12px;
    color: white;
    background-color: #149c54;
    text-align: center;
    cursor: pointer;
    width: 100%;
    font-size: 18px;
    text-transform: uppercase;
}

.md-card button:hover {
    opacity: 0.7;
}

/* START PRODUCT TEMPLATE */
.swiper-container-product-page {
    overflow: hidden;
    width: 100%;
    height: 100%;
}

.product-template {
    padding-top: 25px;
    display: grid;
    grid-template-columns: 50% 50%;
    justify-content: space-between;
    margin-bottom: 32px;
}

#tsum-tabs p {
    margin: 0 0 20px;
    line-height: 1.5;
}

#tsum-tabs main {
    min-width: 320px;
    max-width: 800px;
    padding: 50px;
    margin: 0 auto;
    background: #fff;
}

#tsum-tabs section {
    display: none;
    padding: 20px 0 0;
    border-top: 1px solid #ddd;
    min-height: 10rem;
}

#tsum-tabs input {
    display: none;
}

#tsum-tabs label {
    display: inline-block;
    margin: 0 0 -1px;
    padding: 15px 25px;
    font-family: 'Montserrat-Medium';
    font-weight: 600;
    text-align: center;
    color: #bbb;
    border: 1px solid transparent;
}

#tsum-tabs label:before {
    font-family: fontawesome;
    font-weight: normal;
    margin-right: 10px;
}

#tsum-tabs label[for*='1']:before {
    content: '\f05a';
}

#tsum-tabs label[for*='2']:before {
    content: '\f0ca';
}

#tsum-tabs label[for*='3']:before {
    content: '\f0c3';
}

#tsum-tabs label:hover {
    color: #888;
    cursor: pointer;
}

#tsum-tabs input:checked+label {
    color: #0c5e32;
    border: 1px solid #ddd;
    border-top: 2px solid #0c5e32;
    border-bottom: 1px solid #fff;
}

#tsum-tabs #tab1:checked~#content1,
#tsum-tabs #tab2:checked~#content2,
#tsum-tabs #tab3:checked~#content3 {
    display: block;
}

#tsum-tabs table {
    width: 100%;
    font-family: 'Montserrat-Medium';
    color:#0c5e32;
} 

#tsum-tabs table td {
    padding:1rem;
} 

#tsum-tabs table, #tsum-tabs tr, #tsum-tabs td {
    border: 1px solid #0c5e32;
} 

@media screen and (max-width: 650px) {
    #tsum-tabs label {
        font-size: 0;
    }

    #tsum-tabs label:before {
        margin: 0;
        font-size: 18px;
    }
}

@media screen and (max-width: 400px) {
    #tsum-tabs label {
        padding: 15px;
    }
}

@media (max-width: 900px) {
    .product-template {
        grid-template-columns: 100%;
    }
}

.product-template .swiper-button-next::before {
    background: url('{{"cursor-arrow-right.svg" | asset_url }}') 24px center no-repeat;
    content: "";
    opacity: 0.25;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    transition: opacity 0.15s;
    background-position: calc(100% - 30px);
}

.product-template .swiper-button-prev::before {
    background: url('{{"cursor-arrow-left.svg" | asset_url }}') 24px center no-repeat;
    content: "";
    opacity: 0.25;
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    transition: opacity 0.15s;
}

.product-images:hover .swiper-button-prev::before,
.product-images:hover .swiper-button-next::before {
    opacity: 0;
}

.product-description-container {
    display: flex;
    flex-direction: column;
    padding: 6rem;
}

.product-description {
    width: 100%;
    margin-bottom: 30px;
}

.product-description p {
    font-family: 'Montserrat-Medium';
    letter-spacing: 1px;
    line-height: 1.2;
    font-size: 1.2rem;
    font-weight: 200;
}


@media (max-width: 900px) {
    .product-description {
        width: 100%;
        padding: 0;
    }
}

.product-title {
    margin-bottom: 100px;
    font-size: 30px;
    text-transform: uppercase;
    color: #0c5e32;
    font-family: 'Montserrat-Medium', Arial, sans-serif;
}

.product-option-row {
    position: relative;
    user-select: none;
}

.product-option-label::before {
    content: "";
    width: 11px;
    height: 11px;
    position: absolute;
    right: 25px;
    top: 50%;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 9.5px 5.5px 0 5.5px;
    border-color: #0c5e32 transparent transparent transparent;
}



.product-option-label,
.add-to-cart-button {
    padding: 25px;
    position: relative;
    border: solid;
    cursor: pointer;
    border-color: #bbb;
    border-width: 1px 0 1px 0;
    border-right-color: transparent;
    border-left-color: transparent;
    background-color: white;
    color: #0c5e32;
    font-size: 16px;
    width: 100%;
    text-align: left;
    font-family: 'Montserrat-Medium';
}

.add-to-cart-button {
    text-transform: uppercase;
    background-color: #149c54;
    margin: 2rem 0;
    padding: 25px;
    position: relative;
    border: solid;
    cursor: pointer;
    border-color: black;
    border-width: 0 1px 1px 0;
    border-right-color: transparent;
    border-left-color: transparent;
    font-size: 16px;
    width: 100%;
    color: #fff;
    text-align: center;
    overflow: hidden;
    z-index: 1;
}

.add-to-cart-button:after {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #149c54;
    z-index: -2;
}

.add-to-cart-button:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    width: 0%;
    height: 100%;
    background-color: #0c5e32;
    transition: all 0.3s;
    z-index: -1;
}

.add-to-cart-button:hover {
    color: #fff;
}

.add-to-cart-button:hover:before {
    width: 100%;
}

.add-to-cart-button:hover {
    background-color: black;
    color: white;
}

.product-option-label:hover {
    background: #ededed;
}

.product-option-list {
    position: absolute;
    bottom: 70px;
    right: 0;
    left: 0;
    background-color: white;
    border: solid 1px black;
}

.product-option-item {
    padding: 25px;
    cursor: pointer;
    font-family: 'Montserrat-Medium';
    color: #0c5e32;
}

.product-option-item:nth-child(n+2) {
    box-shadow: inset 0 1px #9a9a9a;
}

.product-option-item:hover {
    background-color: #0c5e32;
    color: white;
}

/* START MINI CART */
.mini-cart.hidden {
    transform: translateX(100%);
}

.mini-cart.hidden .mini-cart-close-button {
    opacity: 0;
    transition: none;
}

.mini-cart.hidden::before {
    opacity: 0;
}

.mini-cart-close-button {
    cursor: pointer;
    position: absolute;
    top: 25px;
    left: 25px;
    right: auto;
    color: inherit;
    transition: opacity 0.5s 0.1s, padding 0.15s;
}

.mini-cart {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    height: 100vh;
    width: 100%;
    z-index: 99;
    max-width: 350px;
    background-color: white;
    padding: 72px 25px 25px 25px;
    transition-property: transform;
}

.mini-cart::before {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    -webkit-box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    transition-property: opacity;
    z-index: -1;
}

@media (max-width: 599px) {
    .mini-cart {
        position: fixed;
        width: calc(100vw - 50px);
        height: 100%;
        /*overflow-x: hidden;
       */
    }
}

.mini-cart,
.mini-cart::before {
    transition-duration: 1s;
    transition-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
}

.cart-form {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.cart-button {
    cursor: pointer;
    position: relative;
    transition: 0.15s;
    z-index: 999;
    color: #0c5e32;
}

.cart-button:hover {
    padding-right: 15px;
}

.cart-button:hover .cart-arrow-icon {
    opacity: 1;
    right: 0;
}

.cart-arrow-icon {
    opacity: 0;
    width: 11.5px;
    height: 12px;
    position: absolute;
    top: 5px;
    right: -15px;
    transition: 0.15s;
    pointer-events: none;
}

.mini-cart-image-wrapper {
    width: 140px;
    margin-right: 25px;
}

.mini-cart-image-wrapper img {
    width: 100%;
}

.mini-cart-rows {
    border-top: 1px solid #0c5e32;
    border-bottom: 1px solid #0c5e32;
    height: 100%;
    overflow: auto;
}

.mini-cart-item {
    display: flex;
    padding: 25px 0;
}

.mini-cart-item-right {
    flex: 1;
}

.mini-cart-item a {
    color: #0c5e32;
}

.mini-cart-quantity {
    height: 25px;
    overflow: hidden;
    margin-top: 25px;
    position: relative;
    border: 1px solid;
    border-radius: 13px;
    color: #0c5e32;
}

.mini-cart-quantity input {
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 25px;
}

.mini-cart-quantity .mini-cart-quantity-button {
    width: 25px;
    position: absolute;
    top: -4px;
    bottom: 0;
    left: 0;
    cursor: pointer;
}

.mini-cart-quantity .mini-cart-quantity-button:last-of-type {
    left: auto;
    right: 0;
}

.checkout-button {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 50px;
    border-radius: 25px;
    padding: 0 25px;
    position: relative;
    transition: 0.15s;
    padding-bottom: 1px;
    background: #149c54;
    color: white;
}

.checkout-button:hover {
    opacity: 0.85;
}

.checkout-subtotal {
    display: flex;
    justify-content: space-between;
    height: 72px;
    padding: 0 25px;
    line-height: 72px;
}

.outside {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
}

.cart-empty {
    opacity: 0.2;
    padding-top: 42px;
    font-size: 24px;
    line-height: 1.5;
}

/* START MENU DRAWER */

.menu-drawer-image-wrap {
    position: relative;
    width: 95%;
    height: auto;
}

.menu-drawer-image-wrap img {
    width: 100%;
    height: 100%;
}

.menu-drawer {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    height: 100vh;
    width: 100%;
    z-index: 99;
    max-width: 350px;
    background-color: white;
    padding: 72px 25px 25px 25px;
    transition: transform 0.9s cubic-bezier(0.165, 0.84, 0.44, 1), left 0.5s 0s;
}

.menu-drawer::before {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    -webkit-box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: 1px 3px 47px 37px rgba(0, 0, 0, 0.25);
    transition-property: opacity;
    z-index: -1;
}

@media (max-width: 599px) {
    .menu-drawer {
        position: fixed;
        width: calc(100vw - 50px);
    }
}

.menu-drawer.hidden {
    transform: translateX(-100%);
}

.menu-drawer.hidden::before {
    opacity: 0;
}

.menu-drawer-inner {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.menu-drawer-inner .navigation-menu a {
    font-size: 1.3rem;
    color: #0c5e32;
}

.menu-drawer-inner .navigation-menu li {
    margin-bottom: 1rem;
}

.menu-button {
    position: relative;
    cursor: pointer;
    transition: 0.15s;
    z-index: 999;
    color: #0c5e32;
}

.menu-button:hover {
    padding-left: 15px;
}

.menu-button:hover .menu-arrow-icon {
    opacity: 1;
    left: 0;
}

.menu-arrow-icon {
    transform: rotate(180deg);
    opacity: 0;
    width: 11.5px;
    height: 12px;
    position: absolute;
    top: 5px;
    left: -15px;
    transition: 0.15s;
    pointer-events: none;
}

/* START FOOTER */
.footer-inner {
    padding-top: 25px;
    border-top: 1px solid black;
}

@media (max-width: 599px) {
    .footer-inner {
        flex-direction: column;
    }
}

.footer-left,
.footer-right {
    width: 100%;
    padding-bottom: 25px;
}

.footer-menu a {
    color: #0c5e32;
}

@media (hover: hover) and (pointer: fine) {
    .footer-menu a {
        transition: color 0.5s;
        font-size: .85rem;
    }

    .footer-menu li a:hover {
        color: #333;
    }
}

/* START HERO SECTION */
@media (max-width: 599px) {
    .hero-image {
        height: 270px;
    }

    .hero-image img {
        height: 100%;
        object-fit: cover;
    }
}

/* START RICH TEXT SECTION */
.heading {
    font-size: 30px;
    font-family: 'Sora-Medium', Arial, sans-serif;
    text-align: left;
}

.center {
    text-align: center;
}

.padding-top {
    padding-top: 25px;
}

/* IMAGE WITH TEXT SECTION */
.image-with-text {
    display: grid;
    grid-template-columns: 1fr 1fr;
}

@media (max-width: 599px) {
    .image-with-text {
        grid-template-columns: 1fr;
    }
}

.image-with-text.reverse {
    direction: rtl;
}

.image-with-text-description-container {
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 25px 25px 25px 10%;
    direction: ltr;
}

.image-with-text-body {
    width: 75%;
    padding-top: 1.5rem;
    text-align: center;
}

.image-with-text-body p {
    letter-spacing: 2px;
    line-height: 1.5;
    font-size: 1.4rem;
    text-align: left;
    font-weight: 300;
    color: #333;
    font-family: 'Sora-Medium',Arial,sans-serif;
}

.iwt-button {
    padding: 1rem;
    background-color: #149c54;
    border-radius: .25rem;
    margin: 1rem 0;
    width: 25%;
    text-align: center;
    color: white;
    text-transform: uppercase;
    font-family: 'Sora-Medium', Arial, sans-serif;
}

/* START LOGO LIST */
.logo-container {
    display: flex;
    justify-content: space-between;
    padding-top: 50px;
}

.logo-link {
    max-width: 25%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.logo-link img {
    width: 50%;
}

/* START COLLECTION FEATURE SECTION */
  
.collection-feature {
    max-width: 95%;
    margin-right: auto;
    margin-left: auto;
    padding: 0 10px;
  }
  
  .collection-feature .row {
    display: flex;
    align-items: center;
  }
  .collection-feature .heading {
    margin-bottom: 2rem;
  }
  @media screen and (max-width: 425px) {
    .collection-feature .row {
      flex-wrap: wrap;
    }
  }
  
  .collection-feature .row p {
    font-family: 'Sora-Medium',Arial,sans-serif;
    color: #333;
    line-height: 1.5;
    font-size: 1.4rem;
    font-weight: 300;
    margin-bottom: 2rem;
  }

  .collection-feature .iwt-button {
    margin: 1rem 0;
  }

  .collection-feature .col {
    width: 50%;
    flex-direction: column;
  }
  @media screen and (max-width: 425px) {
    .collection-feature .col {
      width: 100%;
    }
  }
  
  .collection-feature .col + .collection-feature .col {
    margin-left: 2em;
  }
  @media screen and (max-width: 425px) {
    .collection-feature .col + .collection-feature .col {
      margin-left: 0;
    }
  }

  /* START CTA SECTION */

  .banner h1, .banner h3, .banner h6 {
    margin: 0;
    text-shadow: 2px 2px 6px #000;
    text-align: left;
  }
  
  .banner {
    color: white;
    background: top left/cover no-repeat;
    height: 300px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: 'Sora-Medium',Arial,sans-serif;
  }
  .banner h1, .banner h3, .banner h6 {
    color: white;
  }
  .banner h1 {
    font-size: 60px;
  }
  .banner h3 {
    font-size: 32px;
    font-weight: normal;
    text-transform: uppercase;
    letter-spacing: 4px;
  }
  .banner h6 {
    font-size: 22px;
  }
  .banner p {
    margin-bottom: 2rem;
    font-size: 2rem;
    letter-spacing: 1.2px;
  }
  .banner .iwt-button {
    margin: 1rem 0;
  }

  /* BILLBOARD SECTION */

  .bb .container {
    width: 100%;
    height: 15%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #149c54;
    overflow: auto;
  }
  
  .bb h1 {
    font-size: 10.5vw;
    line-height: 1.05em;
    text-transform: uppercase;
    color: #333;
    letter-spacing: 0.5vw;
    margin: 0;
    /*background: linear-gradient(124deg, #c53d3d 19%, #000 19%, #000 20%, #56b337 20%, #56b337 39%, #000 39%, #000 40%, #3a47d3 40%, #3a47d3 59%, #000 59%, #000 60%, #e1ed2f 60%, #e1ed2f 79%, #000 79%, #000 80%, #c318be 80%, #c318be 100%);*/
    -webkit-background-clip: text;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
    transition: all 0.5s;
  }